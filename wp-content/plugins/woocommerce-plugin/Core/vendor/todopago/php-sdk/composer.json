{
    "name": "todopago/php-sdk",
    "description": "SDK para TodoPago",
    "require": {
        "php": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "TodoPago\\": "TodoPago/lib",
            "TodoPago\\Test\\": "TodoPago/test"
        }
    },
    "repositories": [
        {
             "type": "composer",
             "url": "https://packagist.org"
        },
        { 
            "packagist": false 
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-mock/php-mock-phpunit": "^1.1"
    }
}
